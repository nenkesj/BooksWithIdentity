                        <option value="Denominator" selected>Denominator</option>
                    }
                    else
                    {
                        <option value="Denominator">Denominator</option>
                    }
                </select>
                @Html.Hidden("target", @Model.Target)
                @Html.DropDownListFor(x => x.Block, new SelectList(
                new[]
                {
                "Inline",
                "Block"
                }))
                @Html.Hidden("block", @Model.Block)
                <label>Clear Formula</label><input id="ClearFormula" name="ClearFormula" type="checkbox" value="true" /><input name="ClearFormula" type="hidden" value="false" />
                @Html.Hidden("clearformula", @Model.ClearFormula)
                <label>Reverse</label><input id="Reverse" name="Reverse" type="checkbox" value="true" /><input name="Reverse" type="hidden" value="false" />
                @Html.Hidden("reverse", @Model.Reverse)
                @if (@Model.Target == "Numerator")
                {
                    <label>Clear Numerator</label>

                    <input id="ClearNumerator" name="ClearNumerator" type="checkbox" value="true" />

                    <input name="ClearNumerator" type="hidden" value="false" />
                    @Html.Hidden("clearnumerator", @Model.ClearNumerator)
                }
                @if (@Model.Target == "Denominator")
                {
                    <label>Clear Denominator</label>

                    <input id="ClearDenominator" name="ClearDenominator" type="checkbox" value="true" />

                    <input name="ClearDenominator" type="hidden" value="false" />
                    @Html.Hidden("cleardenominator", @Model.ClearDenominator)
                }
            </div>
            <br />
            <div class="dataElem">
                <table>
                    <tr>
                        <td><label>Identifier 1</label></td>
                        <td>
                            <input asp-for="Ident1" value="@Model.Ident1" class="form-control" />
                        </td>
                        <td><label>Identifier 2</label></td>
                        <td>
                            <input asp-for="Ident2" value="@Model.Ident2" class="form-control" />
                        </td>
                        <td><input id="Id2" name="Id2" type="checkbox" value="true" /><input name="Id2" type="hidden" value="false" /></td>
                        <td><input id="BoldIdent" name="BoldIdent" type="checkbox" value="true" /><input name="BoldIdent" type="hidden" value="false" /></td>
                        <td><label>Bold</label></td>
                        <td><input id="BothIdent" name="BothIdent" type="checkbox" value="true" /><input name="BothIdent" type="hidden" value="false" /></td>
                        <td><label>Both (Inserts both Identifiers)</label></td>

                    </tr>
                    <tr>
                        <td><label>Number 1</label></td>
                        <td>
                            <input asp-for="Num1" value="@Model.Num1" class="form-control" />
                        </td>
                        <td><label>Number 2</label></td>
                        <td>
                            <input asp-for="Num2" value="@Model.Num2" class="form-control" />
                        </td>
                        <td><input id="N2" name="N2" type="checkbox" value="true" /><input name="N2" type="hidden" value="false" /></td>
                        <td><input id="BoldNum" name="BoldNum" type="checkbox" value="true" /><input name="BoldNum" type="hidden" value="false" /></td>
                        <td><label>Bold</label></td>
                        <td><input id="BothNum" name="BothNum" type="checkbox" value="true" /><input name="BothNum" type="hidden" value="false" /></td>
                        <td><label>Both (Inserts Number before Identifier)</label></td>
                    </tr>
                    <tr>
                        <td><label>Operator 1</label></td>
                        <td>
                            <input asp-for="Oper1" value="@Model.Oper1" class="form-control" />
                        </td>
                        <td><label>Operator 2</label></td>
                        <td>
                            <input asp-for="Oper2" value="@Model.Oper2" class="form-control" />
                        </td>
                        <td><input id="Op2" name="Op2" type="checkbox" value="true" /><input name="Op2" type="hidden" value="false" /></td>
                        <td></td>
                        <td></td>
                        <td><input id="BothOper" name="BothOper" type="checkbox" value="true" /><input name="BothOper" type="hidden" value="false" /></td>
                        <td><label>Both (Inserts Both Operators)</label></td>
                    </tr>
                    <tr>
                        <td><label>Space</label></td>
                        <td>
                            <input asp-for="Space" value="@Model.Space" class="form-control" />
                        </td>
                        <td><label>Text</label></td>
                        <td>
                            <input asp-for="Text" value="@Model.Text" class="form-control" />
                        </td>
                        <td></td>
                        <td><input id="BoldText" name="BoldText" type="checkbox" value="true" /><input name="BoldText" type="hidden" value="false" /></td>
                        <td><label>Bold</label></td>
                        <td><input id="BothText" name="BothText" type="checkbox" value="true" /><input name="BothText" type="hidden" value="false" /></td>
                        <td><label>Both (Inserts Space & Text)</label></td>

                    </tr>
                    <tr>
                        <td><label>Row</label></td>
                        <td><input asp-for="Row" value="@Model.Row" class="form-control" /></td>
                        <td><label>Column</label></td>
                        <td><input asp-for="Column" value="@Model.Column" class="form-control" /></td>
                        <td></td>
                        <td></td>
                        <td></td>
                    </tr>
                </table>
            </div>
            <br />
            <input type="submit" value="Insert" class="btn btn-primary" />
            <br />
            <br />
            <div class="dataElem">
                <label>@ViewBag.formula</label><br />
            </div>
        </form>
    </div>
</div>

