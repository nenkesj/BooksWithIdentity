        <h3>Formula</h3>
    </div>

    <div class="panel-body">
        <form asp-action="Edit" asp-controller="Formula" asp-route-undo=false>
            <!--Html.ValidationSummary()-->
            @Html.Hidden("NodeID", @Model.NodeID)
            <div class="dataElem">
                @{
                    var txt = new HtmlString((String)@ViewBag.formula);
                }
                <p>@txt</p>
            </div>
            <br />
            <div class="dataElem">
                <label for="Algebraic">Algebraic:</label>
                <select id="Algebraic" name="Algebraic">
                    <option value="None" selected>None</option>
                    <option value="plus;">+</option>
                    <option value="minus;">-</option>
                    <option value="times;">×</option>
                    <option value="divide;">÷</option>
                    <option value="ne;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&ne;</mo></math></option>
                    <option value="asymp;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&asymp;</mo></math></option>
                    <option value="lt;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&lt;</mo></math></option>
                    <option value="le;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&le;</mo></math></option>
                    <option value="gt;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&gt;</mo></math></option>
                    <option value="ge;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&ge;</mo></math></option>
                    <option value="plusmn;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&plusmn;</mo></math></option>
                    <option value="prop;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&prop;</mo></math></option>
                    <option value="sum;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&sum;</mo></math></option>
                    <option value="prod;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&prod;</mo></math></option>
                    <option value="lfloor;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&lfloor;</mo></math></option>
                    <option value="rfloor;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&rfloor;</mo></math></option>
                    <option value="lceil;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&lceil;</mo></math></option>
                    <option value="rceil;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&rceil;</mo></math></option>
                </select>
                <label for="Calculus">Calculus:</label>
                <select id="Calculus" name="Calculus">
                    <option value="None" selected>None</option>
                    <option value="prime;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&prime;</mo></math></option>
                    <option value="Prime;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&Prime;</mo></math></option>
                    <option value="tprime;"><math xmlns="http://www.w3.org/1998/Math/MathML" display='inline'><mo>&tprime;</mo></math></option>
